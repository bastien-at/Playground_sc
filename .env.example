# URL du webhook n8n (côté serveur uniquement)
N8N_WEBHOOK_URL=https://ton-instance.n8n.cloud/webhook/workflow-execute

# Optionnel (si ton instance n8n le requiert)
N8N_API_KEY=ton_api_key_optionnel

# Optionnel (Basic Auth HTTP)
N8N_BASIC_AUTH_USER=mon_user
N8N_BASIC_AUTH_PASSWORD=mon_password

# Airtable (Report a problem)
AIRTABLE_API_KEY=ton_airtable_api_key
AIRTABLE_BASE_ID=appXXXXXXXXXXXXXX
AIRTABLE_TABLE_NAME=Reports

# Mot de passe d'authentification pour l'application
APP_PASSWORD=votre_mot_de_passe_ici_ou_la
